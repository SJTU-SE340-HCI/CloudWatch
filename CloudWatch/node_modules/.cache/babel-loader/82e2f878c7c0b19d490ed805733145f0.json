this.$store.commit('showMessage', {\r\n            type: 'error',\r\n            message: error.message\r\n          })\r\n          this.showDialog = false\r\n        })\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"stylus\" scoped>\r\n.card {\r\n  background: #fff;\r\n  padding: 12px;\r\n  border-radius: 5px;\r\n  width: 300px;\r\n\r\n  .card-header {\r\n    font-size: 18px;\r\n  }\r\n\r\n  .card-content {\r\n    font-size: 14px;\r\n  }\r\n}\r\n</style>\r\n"],"sourceRoot":"src/components/message/message-elements"}]}